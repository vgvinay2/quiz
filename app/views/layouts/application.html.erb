<!DOCTYPE html>
<html>
<head>
  <title>BookTable</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
<h2> Question List </h2>
<%= link_to("New Question", new_question_questions_path )%>



<%= yield %>


<% if user_signed_in? %>
   <%= "Welcome "%>
<%= link_to "Sign Out",  destroy_user_session_path, :method => :delete  %> | <%= link_to "Click To Edit", edit_user_registration_path %>
<% else%>
<%= link_to "Sign_in", new_user_session_path %> |
<%= link_to "Sign_Up", new_user_registration_path %>
<% end %>

</body>
</html>
